<ul>
{% for breadcrumb_item in knp_menu_get_breadcrumbs_array(knp_menu_get_current_item(item)) %}
    {% if not loop.last %}
        <li><a href="{{ breadcrumb_item.uri }}">{{ breadcrumb_item.label }}</a></li>
    {% else %}
        <li class="active">{{ breadcrumb_item.label }}</li>
    {% endif %}
{% endfor %}
</ul>